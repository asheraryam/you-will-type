script/source = "extends Control

var scores
var anim
const path = \"user://high_scores.p1x\"

func _ready():
	self.scores = self.get_node(\"center/point/labels/scores_score\")
	self.anim = self.get_node(\"anim\")
	self.anim.connect(\"finished\", self, \"move_post_anim\")
	load_scores()
	set_process_input(true)
	
func _input(event):
	if(Input.is_key_pressed(KEY_ESCAPE)):
		get_tree().quit()
	if event.type == InputEvent.KEY and event.is_pressed():
		self.anim.play(\"start\")
	
func move_post_anim():
	var a = self.anim.get_current_animation()
	if a == \"start\":
		start_game()

func start_game():
	get_tree().change_scene(\"res://game.tscn\")

func sorting(a, b):
	if a[1] > b[1]: return true
	else: return false
	
func load_scores():
	var data = get_scores()
	var output = \"\"
	data.sort_custom(self, \"sorting\")
	for score in range(0, 9):
		output += str(data[score][0]) + \"   \" + str(data[score][1]) + \"\\n\"
	self.scores.set_text(output)

func init_scores():
	var file = File.new()
	var data = [
		['P1X',90000],
		['P1X',80000],
		['P1X',70000],
		['P1X',60000],
		['P1X',50000],
		['P1X',40000],
		['P1X',30000],
		['P1X',20000],
		['P1X',10000]]
	
	file.open(self.path, File.WRITE)
	file.store_var(data)
	file.close()

func get_scores():
	var file = File.new()
	if not file.file_exists(self.path): init_scores()
	file.open(self.path, file.READ)
	var data = file.get_var()
	file.close()
	return data
"

